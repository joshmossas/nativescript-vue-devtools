{
	"name": "nativescript-vue-devtools",
	"version": "1.5.0",
	"description": "A NativeScript-Vue plugin for connecting to the standalone vue-devtools",
	"main": "dist/index.js",
	"nativescript": {
		"plugin": {
			"vue": "true"
		}
	},
	"scripts": {
		"build": "tsc",
		"prepublish": "npm run build"
	},
	"files": [
		"dist"
	],
